import java.util.Scanner;

public class H {
    public static void main(String[] args)
    {
        Scanner input=new Scanner(System.in);
        System.out.printf("M=");
        int M=input.nextInt();
        System.out.printf("N=");
        int N=input.nextInt();
        for(int i=1;i<=N;i++)
        {
            System.out.println(i+"x"+M+"="+(i*M));
        }

    }
}
